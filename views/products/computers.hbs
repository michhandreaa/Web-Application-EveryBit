<br />

<main role="main">

    <img class="imgFilter first-slide" src="img/banners/computers.png" width="100%" height="auto" alt="First Slide">

    <br />
    <br />
    <br />

        {{# each products}}

            <div class = "row">

                {{# each this}}

                    <div class="col-md-4">
                        <div class="card mb-4 shadow-sm">
                            <img class="card-img-top" background-size="cover;" style="height:16rem;" src="{{this.imageLink}}" alt="Card image cap">
                            <div class="card-body">
                                <p class="card-text">{{this.title}}</p>
                                <p class="card-text">Price: {{this.price}}.00</p>
                                <div class="d-flex justify-content-between align-items-center">
                                    <div class="btn-group">
                                        <button id="${{!}}" type="button" onclick="cart('${{!}}','${{!}}','${{!URL}}','${{!con}}','${{!btn}}')" class="btn btn-sm btn-outline-secondary" >Add to cart</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                {{/each}}

            </div>

        {{/each}}
   

</main>

<script src="/js/computers.js"></script>